            <zeroOrMore>
               <choice>
                  <ref name="tei_model.lLike"/>
                  <ref name="tei_model.stageLike"/>
                  <ref name="tei_model.labelLike"/>
                  <ref name="tei_model.pPart.transcriptional"/>
                  <ref name="tei_model.global"/>
                  <ref name="tei_lg"/>
               </choice>
            </zeroOrMore>
            <zeroOrMore>
               <group>
                  <ref name="tei_model.divBottom"/>
